{
  "expo": {
    "name": "Bacon Ipsum",
    "slug": "bacon-ipsum",
    "description": "A meatier Lorem Ipsum Generator",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6c0e0f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baconipsum"
    },
    "android": {
      "package": "com.baconipsum"
    },
    "packagerOpts": {
      "nonPersistent": true
    }
  }
}
